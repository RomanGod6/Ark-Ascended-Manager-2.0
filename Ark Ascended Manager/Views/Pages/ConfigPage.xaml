                                            <DataGridTextColumn Header="{Binding Source={StaticResource Strings}, Path=EngramClassName}"
                                    Binding="{Binding EngramClassName}"
                                    Width="2*"/>
                                            <DataGridCheckBoxColumn Header="{Binding Source={StaticResource Strings}, Path=Hidden}"
                                        Binding="{Binding EngramHidden}"
                                        Width="Auto"/>
                                            <DataGridTextColumn Header="{Binding Source={StaticResource Strings}, Path=PointsCost}"
                                    Binding="{Binding EngramPointsCost}"
                                    Width="Auto"/>
                                            <DataGridTextColumn Header="{Binding Source={StaticResource Strings}, Path=LevelRequirement}"
                                    Binding="{Binding EngramLevelRequirement}"
                                    Width="Auto"/>
                                            <DataGridCheckBoxColumn Header="{Binding Source={StaticResource Strings}, Path=AutoUnlock}"
                                        Binding="{Binding AutoUnlock}"
                                        Width="Auto"/>
                                            <DataGridTextColumn Header="{Binding Source={StaticResource Strings}, Path=LevelToAutoUnlock}"
                                    Binding="{Binding LevelToAutoUnlock}"
                                    Width="Auto"/>
                                        </DataGrid.Columns>
                                    </DataGrid>
                                </StackPanel>
                            </Expander>



                            <Expander Header="{Binding Source={StaticResource Strings}, Path=HostedFiles}">
                                <StackPanel PreviewMouseWheel="ScrollViewer_PreviewMouseWheel">

                                </StackPanel>
                            </Expander>
                            <Expander Header="{Binding Source={StaticResource Strings}, Path=StackSizeOverrides}">
                                <StackPanel>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Margin="1">
                                        <Button Content="{Binding Source={StaticResource Strings}, Path=AddRow}" Click="AddRow_Click" Margin="3" Height="28"/>
                                        <Button Content="{Binding Source={StaticResource Strings}, Path=PasteItems}" Click="PasteItems_Click" HorizontalAlignment="Stretch" Margin="3"/>
                                        <Button Content="{Binding Source={StaticResource Strings}, Path=SaveConfig}" Click="SaveConfig_Click"/>
                                    </StackPanel>
                                    <DataGrid x:Name="dgStackSizeOverrides" AutoGenerateColumns="False" CanUserAddRows="False">
                                        <DataGrid.Columns>
                                            <!-- Add this column for the remove button -->
                                            <DataGridTemplateColumn Header="{Binding Source={StaticResource Strings}, Path=Remove}" Width="SizeToCells">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <Button Content="X" Foreground="Red" Click="RemoveRow_Click" DataContext="{Binding}"/>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <DataGridTextColumn Header="{Binding Source={StaticResource Strings}, Path=ItemClassString}" Binding="{Binding ItemClassString}" />
                                            <DataGridTextColumn Header="{Binding Source={StaticResource Strings}, Path=MaxItemQuantity}" Binding="{Binding MaxItemQuantity}" />
                                            <DataGridCheckBoxColumn Header="{Binding Source={StaticResource Strings}, Path=IgnoreMultiplier}" Binding="{Binding IgnoreMultiplier}" />
                                        </DataGrid.Columns>
                                    </DataGrid>
                                </StackPanel>
