<Page x:Class="Ark_Ascended_Manager.Views.Pages.ConfigPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Ark_Ascended_Manager.Views.Pages"
      xmlns:vm="clr-namespace:Ark_Ascended_Manager.ViewModels.Pages"
      xmlns:avalonEdit="http://icsharpcode.net/sharpdevelop/avalonedit"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
      xmlns:helpers="clr-namespace:Ark_Ascended_Manager.Helpers" 
      xmlns:resources="clr-namespace:Ark_Ascended_Manager.Resources"
      
      Title="ConfigPage"
      d:DataContext="{d:DesignInstance Type=vm:ConfigPageViewModel, IsDesignTimeCreatable=True}"

      d:DesignHeight="1281.333"
      d:DesignWidth="800"
      ui:Design.Background="{DynamicResource ApplicationBackgroundBrush}"
      ui:Design.Foreground="{DynamicResource TextFillColorPrimaryBrush}"
      Foreground="{DynamicResource TextFillColorPrimaryBrush}"
      mc:Ignorable="d">

    <Page.Resources>
        <!-- iPhone Style Toggle Switch -->
      
        <Style x:Key="iPhoneToggleSwitchStyle" TargetType="CheckBox">
            <Setter Property="Width" Value="50"/>
            <Setter Property="Height" Value="30"/>
            <Setter Property="Background" Value="#E6E6E6"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="CheckBox">
                        <Border Background="{TemplateBinding Background}" Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" CornerRadius="15">
                            <Grid>
                                <Rectangle x:Name="SwitchBackground" Fill="Transparent" Width="48" Height="28" RadiusX="14" RadiusY="14"/>
                                <Ellipse x:Name="SwitchEllipse" Fill="White" Width="28" Height="28" HorizontalAlignment="Left"/>
                            </Grid>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsChecked" Value="True">
                                <Setter TargetName="SwitchBackground" Property="Fill" Value="#4CD964"/>
                                <Setter TargetName="SwitchEllipse" Property="HorizontalAlignment" Value="Right"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <helpers:ListToStringConverter x:Key="ListToStringConverter" />
        <resources:LocalizedStrings x:Key="Strings" />
    </Page.Resources>

    <!-- Use a Grid as the single child of Page -->
    <Grid>


        <!-- Place all your other elements inside this Grid -->
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <!-- For the header -->
            <RowDefinition Height="*"/>
            <!-- For the tab control -->
        </Grid.RowDefinitions>

        <StackPanel Grid.Row="0" Orientation="Horizontal" HorizontalAlignment="Right">
            <Label Content="{Binding CurrentServerConfig.ProfileName}" 
       FontWeight="Bold" 
       FontSize="15"
       Width="136" 
       Margin="5"/>

            <Button Content="{Binding Source={StaticResource Strings}, Path=StartServer}" Command="{Binding StartServerCommand}" Margin="5"/>
            <Button Content="{Binding Source={StaticResource Strings}, Path=StopServer}" Command="{Binding StopServerCommand}" Margin="5"/>
            <Button Content="{Binding Source={StaticResource Strings}, Path=RestartServer}" Command="{Binding RestartServerCommand}" Margin="5"/>
            <Button Content="{Binding Source={StaticResource Strings}, Path=WipeServer}" Command="{Binding WipeServerCommand}" Margin="5"/>
            <Button Content="{Binding Source={StaticResource Strings}, Path=UpdateServer}" Command="{Binding UpdateServerCommand}" Margin="5"/>
            <Button Content="{Binding Source={StaticResource Strings}, Path=DeleteServer}" Command="{Binding DeleteServerCommand}" Margin="5"/>
            <Button Content="{Binding Source={StaticResource Strings}, Path=RestoreBackup}" Click="RestoreBackUpButton_Click" Margin="5"/>
            <Button Content="{Binding Source={StaticResource Strings}, Path=OpenServerFolder}" Click="OpenServerFolderButton_Click" Margin="5"/>
            <Button Content="{Binding Source={StaticResource Strings}, Path=OpenRCON}" Click="OpenRCON_Click" DataContext="{Binding SelectedServerViewModel}" Margin="5"/>




        </StackPanel>

        <!-- TabControl for Server Settings -->
        <TabControl Grid.Row="1" Margin="10" PreviewMouseWheel="ScrollViewer_PreviewMouseWheel" >
            <TabItem Header="{Binding Source={StaticResource Strings}, Path=ConfigSettings}">
                <Grid>
                    <!-- The Grid will contain both the Button and the ScrollViewer -->
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <!-- Row for the Button -->
                        <RowDefinition Height="*"/>
                        <!-- Row for the ScrollViewer -->
                    </Grid.RowDefinitions>
                    <!-- Save Button -->
                    <Button Content="{Binding Source={StaticResource Strings}, Path=SaveConfigSettings}" 
        Command="{Binding SaveGameIniSettingsCommand}" 
        HorizontalAlignment="Right" 
        VerticalAlignment="Top"
        Margin="10"
        Grid.Row="0"/>
                    <StackPanel Orientation="Horizontal" Grid.Row="0">
                        <TextBox x:Name="SearchBoxGameIni" Width="200" Margin="5"/>
                        <Button Content="{Binding Source={StaticResource Strings}, Path=Search}" 
            Margin="5" 
            Click="SearchButtonGameini_Click"/>
                    </StackPanel>
                    <!-- Save Button -->

                    <!-- ScrollViewer for the rest of the content -->
                    <ScrollViewer Grid.Row="1" Margin="10,5,10,15" PreviewMouseWheel="ScrollViewer_PreviewMouseWheel">
                        <StackPanel x:Name="Gameini" PreviewMouseWheel="ScrollViewer_PreviewMouseWheel">
                            <Expander Header="{Binding Source={StaticResource Strings}, Path=Administration}" PreviewMouseWheel="ScrollViewer_PreviewMouseWheel">
                                <StackPanel PreviewMouseWheel="ScrollViewer_PreviewMouseWheel">
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=ServerName}" ToolTip="Server name to be seen in game"/>
                                    <TextBox Text="{Binding SessionName, Mode=TwoWay}" />
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AdminPassword}" ToolTip="Password to become admin"/>
                                    <TextBox Text="{Binding ServerAdminPassword, Mode=TwoWay}" />
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=MOTD}" ToolTip="Message for users who log in"/>
                                    <TextBox Text="{Binding MOTD, Mode=TwoWay}" Height="209"/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=MOTDInGame}" ToolTip="Specifies in seconds the duration of the displayed message on player log-in." />
                                    <RichTextBox x:Name="richTextPreview" IsReadOnly="True" Height="209"/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=MOTDDuration}" ToolTip="Specifies in seconds the duration of the displayed message on player log-in." />
                                    <TextBox Text="{Binding Duration, Mode=TwoWay}" Height="209"/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=ServerPassword}" ToolTip="Password to join the server"/>
                                    <TextBox Text="{Binding ServerPassword, Mode=TwoWay}" />
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=Banlist}" ToolTip="List of Banned Players"/>
                                    <TextBox Text="{Binding Banlist, Mode=TwoWay}" ToolTip="List of Banned Players"/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=ServerIcon}" ToolTip="Icon That will display on discord bot services."/>
                                    <TextBox Text="{Binding ServerIcon, Mode=TwoWay}" ToolTip="Icon That will display on discord bot services."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=CosmeticWhitelistOverride}" ToolTip="The approved list of cosmetics on your server (use URL)"/>
                                    <TextBox Text="{Binding CosmeticWhitelistOverride, Mode=TwoWay}" ToolTip="The approved list of cosmetics on your server (use URL)"/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=PhotoModeRangeLimit}" ToolTip="Range of camera mode in meters"/>
                                    <TextBox Text="{Binding PhotoModeRangeLimit, Mode=TwoWay}" ToolTip="Range of camera mode in meters"/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=SpectatorPassword}" ToolTip="Password needed for spectator"/>
                                    <TextBox Text="{Binding SpectatorPassword, Mode=TwoWay}" ToolTip="Password needed for spectator"/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AdminLogging}" ToolTip="Enable logging admin commands"/>
                                    <CheckBox IsChecked="{Binding AdminLogging, Mode=TwoWay}" ToolTip="Enable logging admin commands"/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AutoSavePeriodMinutes}" ToolTip="When an auto save is intiated in minutes."/>
                                    <TextBox Text="{Binding AutoSavePeriodMinutes, Mode=TwoWay}" ToolTip="When an auto save is intiated in minutes."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=DisableStructureDecayPvE}" ToolTip="Toggle to enable or disable structure decay in PvE mode."/>
                                    <CheckBox IsChecked="{Binding DisableStructureDecayPvE, Mode=TwoWay}" ToolTip="Toggle to enable or disable structure decay in PvE mode."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=ItemStackSizeMultiplier}" ToolTip="Set the multiplier for item stack sizes."/>
                                    <TextBox Text="{Binding ItemStackSizeMultiplier, Mode=TwoWay}" ToolTip="Enter the multiplier for item stack sizes."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=ServerAutoForceRespawnWildDinosInterval}" ToolTip="Set the interval for automatically respawning wild dinosaurs on the server."/>
                                    <TextBox Text="{Binding ServerAutoForceRespawnWildDinosInterval, Mode=TwoWay}" ToolTip="Enter the interval for automatically respawning wild dinosaurs on the server."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=NPCNetworkStasisRangeScalePlayerCountStart}" ToolTip="Set the player count at which NPC network stasis range scaling starts."/>
                                    <TextBox Text="{Binding NPCNetworkStasisRangeScalePlayerCountStart, Mode=TwoWay}" ToolTip="Enter the player count at which NPC network stasis range scaling starts."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=NPCNetworkStasisRangeScalePlayerCountEnd}" ToolTip="Set the player count at which NPC network stasis range scaling ends."/>
                                    <TextBox Text="{Binding NPCNetworkStasisRangeScalePlayerCountEnd, Mode=TwoWay}" ToolTip="Enter the player count at which NPC network stasis range scaling ends."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=NPCNetworkStasisRangeScalePercentEnd}" ToolTip="Set the percentage at which NPC network stasis range scaling ends."/>
                                    <TextBox Text="{Binding NPCNetworkStasisRangeScalePercentEnd, Mode=TwoWay}" ToolTip="Enter the percentage at which NPC network stasis range scaling ends."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=ForceFlyerExplosives}" ToolTip="Toggle to enable or disable the use of explosives by flyers."/>
                                    <CheckBox IsChecked="{Binding ForceFlyerExplosives, Mode=TwoWay}" ToolTip="Toggle to enable or disable the use of explosives by flyers."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=EnableExtraStructurePreventionVolumes}" ToolTip="Toggle to enable or disable extra structure prevention volumes."/>
                                    <CheckBox IsChecked="{Binding EnableExtraStructurePreventionVolumes, Mode=TwoWay}" ToolTip="Toggle to enable or disable extra structure prevention volumes."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=ShowFloatingDamageText}" ToolTip="Toggle to show or hide floating damage text in the game."/>
                                    <CheckBox IsChecked="{Binding ShowFloatingDamageText, Mode=TwoWay}" ToolTip="Toggle to show or hide floating damage text."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AllowHitMarkers}" ToolTip="Toggle to allow or disallow hit markers in the game."/>
                                    <CheckBox IsChecked="{Binding AllowHitMarkers, Mode=TwoWay}" ToolTip="Toggle to allow or disallow hit markers."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=ServerPvE}" ToolTip="Toggle to set the server mode to PvE."/>
                                    <CheckBox IsChecked="{Binding ServerPvE, Mode=TwoWay}" ToolTip="Toggle to set the server mode to PvE."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=DisablePhotoMode}" ToolTip="Toggle to enable or disable photo mode in the game."/>
                                    <CheckBox IsChecked="{Binding DisablePhotoMode, Mode=TwoWay}" ToolTip="Toggle to enable or disable photo mode."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=PreventSpawnAnimations}" ToolTip="If enabled turns off spawn animations when spawning."/>
                                    <CheckBox IsChecked="{Binding PreventSpawnAnimations, Mode=TwoWay}" ToolTip="If enabled turns off spawn animations when spawning."/>
                                    <Expander Header="{Binding Source={StaticResource Strings}, Path=CryopodSettings}" ToolTip="Settings for wildcard cryopods (Not Mods)">
                                        <StackPanel>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=DisableCryopodEnemyCheck}" ToolTip="Will ignore the enemy player/structure/dino checks."/>
                                            <CheckBox IsChecked="{Binding DisableCryopodEnemyCheck, Mode=TwoWay}" ToolTip="Will ignore the enemy player/structure/dino checks."/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=DisableCryopodFridgeRequirement}" ToolTip="will disable the Cryofridge requirement for releasing cryopodded dinos"/>
                                            <CheckBox IsChecked="{Binding DisableCryopodFridgeRequirement, Mode=TwoWay}" ToolTip="will disable the Cryofridge requirement for releasing cryopodded dinos"/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=AllowCryoFridgeOnSaddle}" ToolTip="Will allow Cryofridge to be placed on rafts/saddles "/>
                                            <CheckBox IsChecked="{Binding AllowCryoFridgeOnSaddle, Mode=TwoWay}" ToolTip="Will allow Cryofridge to be placed on rafts/saddles "/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=DestroyTamesOverTheSoftTameLimit}" ToolTip="Enables destroying tames over the limit"/>
                                            <CheckBox IsChecked="{Binding DestroyTamesOverTheSoftTameLimit, Mode=TwoWay}" ToolTip="Enables destroying tames over the limit"/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=MaxTamedDinosSoftTameLimit}" ToolTip="Dinos above the Soft Tame Server Limit will be marked “For Cryo” and display an icon and a timer indicating how soon they need to be cryopodded before they are automatically destroyed."/>
                                            <TextBox Text="{Binding MaxTamedDinosSoftTameLimit, Mode=TwoWay}" ToolTip="Dinos above the Soft Tame Server Limit will be marked “For Cryo” and display an icon and a timer indicating how soon they need to be cryopodded before they are automatically destroyed."/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=MaxTamedDinosSoftTameLimitCountdownForDeletionDuration}" ToolTip="set the timer for destroy in seconds"/>
                                            <TextBox Text="{Binding MaxTamedDinosSoftTameLimitCountdownForDeletionDuration, Mode=TwoWay}" ToolTip="set the timer for destroy in seconds"/>
                                        </StackPanel>
                                    </Expander>
                                </StackPanel>
                            </Expander>
                            <Expander Header="{Binding Source={StaticResource Strings}, Path=Rules}">
                                <StackPanel PreviewMouseWheel="ScrollViewer_PreviewMouseWheel">
