                                    <Label Content="{Binding Source={StaticResource Strings}, Path=BattleNumOfTribestoStartGame}"/>
                                    <TextBox Text="{Binding BattleNumOfTribestoStartGame, Mode=TwoWay}"/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=TimeToCollapseROD}"/>
                                    <TextBox Text="{Binding TimeToCollapseROD, Mode=TwoWay}"/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=BattleAutoStartGameInterval}"/>
                                    <TextBox Text="{Binding BattleAutoStartGameInterval, Mode=TwoWay}"/>
