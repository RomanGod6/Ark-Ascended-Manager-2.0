                                        <StackPanel>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=NoTributeDownloads}" ToolTip="Toggle to prevent or allow tribute downloads between clusters."/>
                                            <CheckBox IsChecked="{Binding NoTributeDownloads, Mode=TwoWay}" ToolTip="Toggle to prevent or allow tribute downloads between clusters."/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=PreventDownloadSurvivors}" ToolTip="Toggle to prevent or allow downloading survivors between clusters."/>
                                            <CheckBox IsChecked="{Binding PreventDownloadSurvivors, Mode=TwoWay}" ToolTip="Toggle to prevent or allow downloading survivors between clusters."/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=PreventDownloadItems}" ToolTip="Toggle to prevent or allow downloading items between clusters."/>
                                            <CheckBox IsChecked="{Binding PreventDownloadItems, Mode=TwoWay}" ToolTip="Toggle to prevent or allow downloading items between clusters."/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=PreventDownloadDinos}" ToolTip="Toggle to prevent or allow downloading dinosaurs between clusters."/>
                                            <CheckBox IsChecked="{Binding PreventDownloadDinos, Mode=TwoWay}" ToolTip="Toggle to prevent or allow downloading dinosaurs between clusters."/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=TributeCharacterExpirationSeconds}" ToolTip="When characters would expire in upload."/>
                                            <TextBox Text="{Binding TributeCharacterExpirationSeconds, Mode=TwoWay}" ToolTip="When characters would expire in upload."/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=TributeDinoExpirationSeconds}" ToolTip="When tributes would expire in upload."/>
                                            <TextBox Text="{Binding TributeDinoExpirationSeconds, Mode=TwoWay}" ToolTip="When tributes would expire in upload."/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=TributeItemExpirationSeconds}" ToolTip="When items would expire in upload."/>
                                            <TextBox Text="{Binding TributeItemExpirationSeconds, Mode=TwoWay}" ToolTip="When items would expire in upload."/>
                                        </StackPanel>
                                    </Expander>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=EnableHardcoreMode}" ToolTip="Enabling this mode will make the game more challenging by increasing the difficulty of survival."/>
                                    <CheckBox IsChecked="{Binding ServerHardcore, Mode=TwoWay}" ToolTip="Toggle to enable or disable Hardcore Mode."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=DisablePvPFriendlyFire}" ToolTip="Disable friendly fire in Player vs. Player combat."/>
                                    <CheckBox IsChecked="{Binding BPvPDisableFriendlyFire, Mode=TwoWay}" ToolTip="Toggle to enable or disable friendly fire in PvP."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=DisablePvEFriendlyFire}" ToolTip="Disable friendly fire in Player vs. Environment scenarios."/>
                                    <CheckBox IsChecked="{Binding BPvEDisableFriendlyFire, Mode=TwoWay}" ToolTip="Toggle to enable or disable friendly fire in PvE."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=DisableLootCrates}" ToolTip="Toggle to enable or disable loot crates in the game."/>
                                    <CheckBox IsChecked="{Binding DisableLootCrates, Mode=TwoWay}" ToolTip="Toggle to enable or disable the spawning of loot crates."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AllowCaveBuildingPvE}" ToolTip="Toggle to allow or disallow building in caves in PvE mode."/>
                                    <CheckBox IsChecked="{Binding AllowCaveBuildingPvE, Mode=TwoWay}" ToolTip="Toggle to allow or disallow building structures in caves in PvE mode."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AllowCaveBuildingPvP}" ToolTip="Toggle to allow or disallow building in caves in PvP mode."/>
                                    <CheckBox IsChecked="{Binding AllowCaveBuildingPvP, Mode=TwoWay}" ToolTip="Toggle to allow or disallow building structures in caves in PvP mode."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=EnableSingleplayerSettings}" ToolTip="Toggle to enable or disable Singleplayer settings. This auto increases certain rates to make it easier for smaller tribes."/>
                                    <CheckBox IsChecked="{Binding UseSingleplayerSettings, Mode=TwoWay}" ToolTip="Toggle to enable or disable Singleplayer settings. This auto increases certain rates to make it easier for smaller tribes."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AllowCrateSpawnsOnTopOfStructures}" ToolTip="Toggle to allow or disallow crate spawns on top of structures."/>
                                    <CheckBox IsChecked="{Binding AllowCrateSpawnsOnTopOfStructures, Mode=TwoWay}" ToolTip="Toggle to allow or disallow crate spawns on top of structures."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=EnableCreativeMode}" ToolTip="Toggle to enable or disable Creative Mode."/>
                                    <CheckBox IsChecked="{Binding ShowCreativeMode, Mode=TwoWay}" ToolTip="Toggle to enable or disable Creative Mode."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=EnableDiseases}" ToolTip="Toggle to enable or disable diseases in the game."/>
                                    <CheckBox IsChecked="{Binding PreventDiseases, Mode=TwoWay}" ToolTip="Toggle to enable or disable diseases."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=NonPermanentDiseases}" ToolTip="Toggle to make diseases non-permanent."/>
                                    <CheckBox IsChecked="{Binding NonPermanentDiseases, Mode=TwoWay}" ToolTip="Toggle to make diseases non-permanent."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=RandomSupplyCratePoints}" ToolTip="Toggle to enable or disable random supply crate spawn points."/>
                                    <CheckBox IsChecked="{Binding RandomSupplyCratePoints, Mode=TwoWay}" ToolTip="Toggle to enable or disable random supply crate spawn points."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=SupplyCrateLootQualityMultiplier}" ToolTip="Set the multiplier for supply crate loot quality."/>
                                    <TextBox Text="{Binding SupplyCrateLootQualityMultiplier, Mode=TwoWay}" ToolTip="Enter the multiplier for supply crate loot quality."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=FishingLootQualityMultiplier}" ToolTip="Set the multiplier for fishing loot quality."/>
                                    <TextBox Text="{Binding FishingLootQualityMultiplier, Mode=TwoWay}" ToolTip="Enter the multiplier for fishing loot quality."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AllowUnlimitedRespec}" ToolTip="Toggle to allow unlimited respecs for characters."/>
                                    <CheckBox IsChecked="{Binding AllowUnlimitedRespec, Mode=TwoWay}" ToolTip="Toggle to allow unlimited character respecs."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AllowPlatformSaddleMultiFloors}" ToolTip="Toggle to allow or disallow multiple floors on platform saddles."/>
                                    <CheckBox IsChecked="{Binding AllowPlatformSaddleMultiFloors, Mode=TwoWay}" ToolTip="Toggle to allow or disallow multiple floors on platform saddles."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=MaxPlatformSaddleStructureLimit}" ToolTip="Set the maximum structure limit for platform saddles."/>
                                    <TextBox Text="{Binding MaxPlatformSaddleStructureLimit, Mode=TwoWay}" ToolTip="Enter the maximum structure limit for platform saddles."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=MaxDifficulty}" ToolTip="Instead of fixed difficulties such as Easy, Medium, and Hard, the difficulty is expressed using a number currently between 0.5 and 4.0[1] (or 5.0 in The Center). "/>
                                    <TextBox Text="{Binding MaxDifficulty, Mode=TwoWay}" ToolTip="Instead of fixed difficulties such as Easy, Medium, and Hard, the difficulty is expressed using a number currently between 0.5 and 4.0[1] (or 5.0 in The Center). "/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=IncreasePvPRespawnIntervalCheckPeriod}" ToolTip="Set the check period for increasing PvP respawn interval."/>
                                    <TextBox Text="{Binding IncreasePvPRespawnIntervalCheckPeriod, Mode=TwoWay}" ToolTip="Enter the check period for increasing PvP respawn interval."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=IncreasePvPRespawnIntervalMultiplier}" ToolTip="Set the multiplier for increasing PvP respawn interval."/>
                                    <TextBox Text="{Binding IncreasePvPRespawnIntervalMultiplier, Mode=TwoWay}" ToolTip="Enter the multiplier for increasing PvP respawn interval."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=IncreasePvPRespawnIntervalBaseAmount}" ToolTip="Set the base amount for increasing PvP respawn interval."/>
                                    <TextBox Text="{Binding IncreasePvPRespawnIntervalBaseAmount, Mode=TwoWay}" ToolTip="Enter the base amount for increasing PvP respawn interval."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=MaxAlliancesPerTribe}" ToolTip="Set the maximum number of alliances a tribe can have."/>
                                    <TextBox Text="{Binding MaxAlliancesPerTribe, Mode=TwoWay}" ToolTip="Enter the maximum number of alliances a tribe can have."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=MaxTribesPerAlliance}" ToolTip="Set the maximum number of tribes allowed in a single alliance."/>
                                    <TextBox Text="{Binding MaxTribesPerAlliance, Mode=TwoWay}" ToolTip="Enter the maximum number of tribes allowed in a single alliance."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=IncreasePvPRespawnInterval}" ToolTip="Toggle to enable or disable increasing PvP respawn interval."/>
                                    <CheckBox IsChecked="{Binding IncreasePvPRespawnInterval, Mode=TwoWay}" ToolTip="Toggle to enable or disable increasing PvP respawn interval."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=PreventOfflinePvPInterval}" ToolTip="Set the interval for preventing offline PvP attacks."/>
                                    <TextBox Text="{Binding PreventOfflinePvPInterval, Mode=TwoWay}" ToolTip="Enter the interval for preventing offline PvP attacks."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=PreventOfflinePvP}" ToolTip="Toggle to prevent or allow offline PvP attacks."/>
                                    <CheckBox IsChecked="{Binding PreventOfflinePvP, Mode=TwoWay}" ToolTip="Toggle to prevent or allow offline PvP attacks."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AutoPvEStartTimeSeconds}" ToolTip="Set the start time for AutoPvE in seconds."/>
                                    <TextBox Text="{Binding AutoPvEStartTimeSeconds, Mode=TwoWay}" ToolTip="Enter the start time for AutoPvE in seconds."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AutoPvEStopTimeSeconds}" ToolTip="Set the stop time for AutoPvE in seconds."/>
                                    <TextBox Text="{Binding AutoPvEStopTimeSeconds, Mode=TwoWay}" ToolTip="Enter the stop time for AutoPvE in seconds."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=MaxNumberOfPlayersInTribe}" ToolTip="Set the maximum number of players allowed in a tribe."/>
                                    <TextBox Text="{Binding MaxNumberOfPlayersInTribe, Mode=TwoWay}" ToolTip="Enter the maximum number of players allowed in a tribe."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=TribeNameChangeCooldown}" ToolTip="Set the cooldown period for changing tribe names."/>
                                    <TextBox Text="{Binding TribeNameChangeCooldown, Mode=TwoWay}" ToolTip="Enter the cooldown period for changing tribe names."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=TribeSlotReuseCooldown}" ToolTip="Set the cooldown period for reusing tribe slots."/>
                                    <TextBox Text="{Binding TribeSlotReuseCooldown, Mode=TwoWay}" ToolTip="Enter the cooldown period for reusing tribe slots."/>
                                    <Expander Header="{Binding Source={StaticResource Strings}, Path=PvETribeWarfareOptions}">
                                        <StackPanel>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=PvEAllowTribeWar}" ToolTip="Allow wars in PVE"/>
                                            <CheckBox IsChecked="{Binding PvEAllowTribeWar, Mode=TwoWay}" ToolTip="Allow wars in PVE"/>
                                            <Label Content="{Binding Source={StaticResource Strings}, Path=PvEAllowTribeWarCancel}" ToolTip="Allow wars in to be cancelled in PVE"/>
                                            <CheckBox IsChecked="{Binding PvEAllowTribeWarCancel, Mode=TwoWay}" ToolTip="Allow wars in to be cancelled in PVE"/>
                                        </StackPanel>
                                    </Expander>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=AllowCustomRecipes}" ToolTip="Toggle to allow or disallow custom recipes in the game."/>
                                    <CheckBox IsChecked="{Binding AllowCustomRecipes, Mode=TwoWay}" ToolTip="Toggle to allow or disallow custom recipes."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=CustomRecipeEffectivenessMultiplier}" ToolTip="Set the multiplier for custom recipe effectiveness."/>
                                    <TextBox Text="{Binding CustomRecipeEffectivenessMultiplier, Mode=TwoWay}" ToolTip="Enter the multiplier for custom recipe effectiveness."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=CustomRecipeSkillMultiplier}" ToolTip="Set the multiplier for custom recipe skill."/>
                                    <TextBox Text="{Binding CustomRecipeSkillMultiplier, Mode=TwoWay}" ToolTip="Enter the multiplier for custom recipe skill."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=NPCNetworkStasisRangeScalePlayerCountStart}" ToolTip="Set the player count at which NPC network stasis range scaling starts."/>
                                    <TextBox Text="{Binding NPCNetworkStasisRangeScalePlayerCountStart, Mode=TwoWay}" ToolTip="Enter the player count at which NPC network stasis range scaling starts."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=NPCNetworkStasisRangeScalePlayerCountEnd}" ToolTip="Set the player count at which NPC network stasis range scaling ends."/>
                                    <TextBox Text="{Binding NPCNetworkStasisRangeScalePlayerCountEnd, Mode=TwoWay}" ToolTip="Enter the player count at which NPC network stasis range scaling ends."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=NPCNetworkStasisRangeScalePercentEnd}" ToolTip="Set the percentage at which NPC network stasis range scaling ends."/>
                                    <TextBox Text="{Binding NPCNetworkStasisRangeScalePercentEnd, Mode=TwoWay}" ToolTip="Enter the percentage at which NPC network stasis range scaling ends."/>
                                    <Label Content="{Binding Source={StaticResource Strings}, Path=OxygenSwimSpeedStatMultiplier}" ToolTip="Set the multiplier for oxygen swim speed stat."/>
