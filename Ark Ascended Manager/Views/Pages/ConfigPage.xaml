                </Grid>
            </TabItem>

            <TabItem Header="{Binding Source={StaticResource Strings}, Path=CustomGameIni}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <!-- Paste Area -->
                    <TextBlock Grid.Row="0" Text="{Binding Source={StaticResource Strings}, Path=PasteGameIniContent}" Margin="5"/>
                    <TextBox Grid.Row="1" 
            Text="{Binding GameIniContent, UpdateSourceTrigger=PropertyChanged}" 
            AcceptsReturn="True" 
            VerticalScrollBarVisibility="Auto"
            TextWrapping="Wrap" 
            Margin="5,23,5,35" Grid.RowSpan="2"/>

                    <!-- INI Sections List -->
                    <ListView Grid.Row="2" 
            ItemsSource="{Binding GameIniSections}" 
            SelectedItem="{Binding SelectedGameIniSection}" 
            Height="200" 
            Margin="5" PreviewMouseWheel="ScrollViewer_PreviewMouseWheel">
                        <ListView.View>
                            <GridView>
                                <GridViewColumn Header="Section" 
                        DisplayMemberBinding="{Binding Header}"/>
                                <GridViewColumn Header="Settings" 
                        DisplayMemberBinding="{Binding SettingsAsString}"/>
                            </GridView>
                        </ListView.View>
                    </ListView>

                    <!-- Save Button -->
                    <Button 
                Content="{Binding Source={StaticResource Strings}, Path=SaveToGameIni}" 
                Command="{Binding SaveGAMEIniFileCommand}" 
                Margin="0,10,24,0" 
                HorizontalAlignment="Right" Grid.RowSpan="2" VerticalAlignment="Top"/>
                </Grid>
            </TabItem>




            <TabItem Header="{Binding Source={StaticResource Strings}, Path=CustomGameUserSettingsIni}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>

                    <TextBox Grid.Row="0"
                     Text="{Binding IniContent, UpdateSourceTrigger=PropertyChanged}"
                     AcceptsReturn="True"
                     VerticalScrollBarVisibility="Auto"
                     HorizontalScrollBarVisibility="Auto"
                     FontFamily="Consolas"
                     Padding="10" Margin="0,48,0,0" PreviewMouseWheel="ScrollViewer_PreviewMouseWheel"/>

                    <Button
                Content="{Binding Source={StaticResource Strings}, Path=Save}"
                Command="{Binding SaveIniFileCommand}"
                HorizontalAlignment="Right"
                Margin="0,10,24,0" VerticalAlignment="Top" Width="105" />
                </Grid>
            </TabItem>










            <TabItem Header="{Binding Source={StaticResource Strings}, Path=LaunchParameters}">
                <!-- Content for Launch Parameters -->
                <Grid>
                    <ScrollViewer VerticalScrollBarVisibility="Auto" PreviewMouseWheel="ScrollViewer_PreviewMouseWheel">
                        <StackPanel Margin="20">
                            <Button Content="{Binding Source={StaticResource Strings}, Path=SaveLaunchParameters}" Margin="20" HorizontalAlignment="Right" Command="{Binding LoadLaunchServerSettingsCommand}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=ServerPlatformSetting}" />
                            <TextBox 
                    Text="{Binding ServerPlatformSetting, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                    ToolTip="{Binding Source={StaticResource Strings}, Path=ServerPlatformSettingTooltip}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=CommandLine}" />
                            <TextBlock x:Name="BatchFilePreview" TextWrapping="Wrap" FontFamily="Consolas" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=OfficialMapSelection}" />
                            <ComboBox x:Name="MapSelectionComboBox"
                          ItemsSource="{Binding OptionsList}" 
                          SelectedValue="{Binding SelectedOption}"
                          SelectedValuePath="Key"
                          DisplayMemberPath="Value" />

                            <CheckBox x:Name="OverrideCheckBox" Content="{Binding Source={StaticResource Strings}, Path=OverrideMapName}" Checked="OverrideCheckBox_Checked" Unchecked="OverrideCheckBox_Unchecked" Margin="5" ToolTip="{Binding Source={StaticResource Strings}, Path=OverrideMapNameTooltip}" />
                            <TextBox x:Name="OverrideTextBox" Visibility="Collapsed" Text="{Binding OverrideMapName}" Margin="5" ToolTip="{Binding Source={StaticResource Strings}, Path=OverrideMapNameInputTooltip}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=ListenPort}" />
                            <TextBox Text="{Binding ListenPort, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=RCONPort}" />
                            <TextBox Text="{Binding RconPort, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=Mods}" />
                            <TextBox Text="{Binding Mods, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                            <CheckBox x:Name="OverrideModCheckBox" Content="{Binding Source={StaticResource Strings}, Path=AddPassiveMod}" Checked="OverrideModCheckBox_Checked" Unchecked="OverrideModCheckBox_Unchecked" Margin="5" ToolTip="{Binding Source={StaticResource Strings}, Path=AddPassiveModTooltip}" />
                            <TextBox x:Name="OverrideModTextBox" Visibility="Collapsed" Text="{Binding OverridePassiveMod}" Margin="5" ToolTip="{Binding Source={StaticResource Strings}, Path=OverridePassiveModTooltip}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=MaxPlayerCount}" />
                            <TextBox Text="{Binding MaxPlayerCount, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=UseBattleye}" />
                            <CheckBox IsChecked="{Binding UseBattleye, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=UseOldConsole}" ToolTip="{Binding Source={StaticResource Strings}, Path=UseOldConsoleTooltip}" />
                            <CheckBox IsChecked="{Binding UseOldConsole, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=DisableCustomCosmetics}" ToolTip="{Binding Source={StaticResource Strings}, Path=DisableCustomCosmeticsTooltip}" />
                            <CheckBox IsChecked="{Binding DisableCustomCosmetics, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=AutoDestroyStructures}" ToolTip="{Binding Source={StaticResource Strings}, Path=AutoDestroyStructuresTooltip}" />
                            <CheckBox IsChecked="{Binding AutoDestroyStructures, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=NotifyAdminCommandsInChat}" ToolTip="{Binding Source={StaticResource Strings}, Path=NotifyAdminCommandsInChatTooltip}" />
                            <CheckBox IsChecked="{Binding NotifyAdminCommandsInChat, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=ForceRespawnDinos}" />
                            <CheckBox IsChecked="{Binding ForceRespawnDinos, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=CustomLaunchOptions}" />
                            <TextBox Text="{Binding CustomLaunchOptions, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Source={StaticResource Strings}, Path=CustomLaunchOptionsTooltip}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=ClusterID}" />
                            <TextBox Text="{Binding ClusterID, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Source={StaticResource Strings}, Path=ClusterIDTooltip}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=ClusterDirOverride}" />
                            <TextBox Text="{Binding ClusterDirOverride, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Source={StaticResource Strings}, Path=ClusterDirOverrideTooltip}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=EnablePlugins}" />
                            <CheckBox IsChecked="{Binding PluginsEnabled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=MultihomeIP}" ToolTip="{Binding Source={StaticResource Strings}, Path=MultihomeIPTooltip}" />
                            <TextBox Text="{Binding MultihomeIP, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <Label Content="{Binding Source={StaticResource Strings}, Path=ServerIP}" ToolTip="{Binding Source={StaticResource Strings}, Path=ServerIPTooltip}" />
                            <TextBox Text="{Binding ServerIP, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                        </StackPanel>
                    </ScrollViewer>
                    <!-- Put the controls related to Launch Parameters here -->
                </Grid>
            </TabItem>




            <TabItem Header="Plugin Management">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="200" />
                        <!-- Width for the ListBox -->
                        <ColumnDefinition Width="Auto" />
                        <!-- Width for the Save Button -->
                        <ColumnDefinition Width="*" />
                        <!-- Remaining width for the AvalonEdit control -->
                    </Grid.ColumnDefinitions>

                    <!-- ListBox to display list of JSON files -->
                    <ListBox x:Name="lstPlugins" Grid.Column="0" 
                 VerticalAlignment="Top" 
                 Height="Auto" SelectionChanged="LstPlugins_SelectionChanged" PreviewMouseWheel="ScrollViewer_PreviewMouseWheel">
                        <!-- ItemTemplate can be customized as needed -->
                    </ListBox>

                    <!-- Save Button -->
                    <Button Content="Save" Grid.Column="1"
                Click="SaveJsonButton_Click" 
                HorizontalAlignment="Center" 
                VerticalAlignment="Top" 
                Padding="5" Margin="10" />

                    <!-- AvalonEdit control for JSON editing -->
                    <avalonEdit:TextEditor x:Name="jsonEditor" Grid.Column="2" 
                               SyntaxHighlighting="JavaScript"
                               HorizontalScrollBarVisibility="Auto" 
                               VerticalScrollBarVisibility="Auto"
                               FontFamily="Consolas" FontSize="22" 
                               Background="#FF3D3D40"
                               Foreground="White"
                               ShowLineNumbers="True" />

                </Grid>
            </TabItem>





        </TabControl>
    </Grid>
</Page>
